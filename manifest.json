{
    "manifest_version": 2,
    "name": "history gateway",
    "author": "gold holk",
    "version": "2.0",
    "description": "filter some history record and save to other place.",
    "homepage_url": "http://github.com/gholk/history-gateway/",
    "icons": {
        "48": "icon-48.png"
    },
    "permissions": ["history", "storage"],
    "background": {
        "scripts": ["lib.js", "background-script.js"]
    },
    "options_ui": {
        "browser_style": false,
        "page": "control-panel.html"
    },
    "omnibox": {
        "keyword": "!hgate"
    }
}
